{
  "manifest_version": 3,
  "name": "Expert Goggles",
  "description": "Guides for Data Visualizations",
  "version": "1.0",
  "background":
  {
    "js": ["Javascript/DBConn.js"]
  },
  "content_scripts":
  [{
      "matches":
      [
        "<all_urls>"
      ],
      "css": ["style/style.css"],
      "js": ["Javascript/UIGen.js"]
  }],
  "icons":
  {
    "128": "icon.png"
  },
  "permissions":
  [
    "unlimitedStorage",
    "notifications"
  ]
}