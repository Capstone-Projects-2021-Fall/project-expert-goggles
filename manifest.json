{
  "manifest_version": 2,
  "name": "Expert Goggles",
  "description": "Guides for Data Visualizations",
  "version": "1.0",
  "background":
  {
    "scripts": ["Javascript/DBConn.js"]
  },
  "content_scripts":
  [{
      "matches": ["<all_urls>"],
      "js": ["Javascript/D3Loader.js"],
      "run_at": "document_start"
   },
   {
      "matches": ["<all_urls>"],
      "css": ["style/style.css"],
      "js":
      [
          "Javascript/UIGen.js"
      ]
  }],
  "web_accessible_resources":
  [
    "Javascript/test.js",
    "Javascript/d3.js"
  ],
  "icons":
  {
    "128": "icon.png"
  },
  "permissions":
  [
    "storage",
    "tabs"
  ]
}