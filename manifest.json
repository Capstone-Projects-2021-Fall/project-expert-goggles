{
  "manifest_version": 2,
  "name": "Expert Goggles",
  "description": "Guides for Data Visualizations",
  "version": "1.0",
  "background":
  {
    "scripts": ["Javascript/DBConn.js"]
  },
  "content_scripts":
  [{
      "matches":
      [
        "<all_urls>"
      ],
      "css": ["style/style.css"],
      "js":
      [
          "Javascript/UIGen.js",
          "Javascript/Scraper.js"
      ]
  }],
  "icons":
  {
    "128": "icon.png"
  },
  "permissions":
  [
    "storage",
    "tabs"
  ]
}