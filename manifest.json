{
  "manifest_version": 2,
  "name": "Expert Goggles",
  "description": "Guides for Data Visualizations",
  "version": "1.0",
  "background":
  {
    "page": "HTML/background.html",
    "persisent": false
  },
  "content_scripts":
  [{
      "matches":
      [
        "<all_urls>"
      ],
      "css": ["style/style.css"],
      "js":
      [
          "Javascript/UIGen.js",
          "Javascript/Scraper.js"
      ]
  }],
  "content_security_policy": "script-src 'self' https://www.gstatic.com/ https://*.firebaseio.com https://www.googleapis.com/; object-src 'self'",
  "icons":
  {
    "128": "icon.png"
  },
  "permissions":
  [
    "storage",
    "tabs"
  ]
}